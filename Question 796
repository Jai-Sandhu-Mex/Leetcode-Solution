class Solution {
    public boolean rotateString(String s, String goal) {
        int n = s.length();
        if (n != goal.length()) {
            return false;
        }
        for(int i=0 ; i<n ; i++){
            s = s.substring(1) + s.charAt(0);  
            if(s.equals(goal)){
                return true;
            }
        }
        return false;
    }
}
